{
    "name": "patrickbussmann/oauth2-apple",
    "description": "Sign in with Apple OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Bu√ümann",
            "email": "patrick.bussmann@bussmann-it.de",
            "homepage": "https://github.com/patrickbussmann"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "apple",
        "sign-in-with-apple"
    ],
    "require": {
        "league/oauth2-client": "^2.0",
        "web-token/jwt-core": "^2.0",
        "web-token/jwt-signature-algorithm-ecdsa": "^2.0",
        "web-token/jwt-key-mgmt": "^2.0",
        "ext-json": "*",
        "web-token/jwt-signature-algorithm-rsa": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
